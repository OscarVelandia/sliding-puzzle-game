{
  "name": "sliding-puzzle-game",
  "version": "1.0.0",
  "description": "Puzzle game for MediaMonks",
  "scripts": {
    "start": "serve build",
    "dev": "NODE_ENV=development parcel src/pages/GameBoard.html --open",
    "build": "NODE_ENV=production parcel build src/pages/GameBoard.html -d build -o index.html --public-url ./",
    "clear-build-cache": "rm -rf dist/ .cache/"
  },
  "author": "Oscar Velandia",
  "repository": {
    "type": "git",
    "utl": "https://github.com/OscarVelandia/sliding-puzzle-game"
  },
  "license": "ISC",
  "dependencies": {
    "serve": "^11.3.2"
  },
  "devDependencies": {
    "eslint": "^7.5.0",
    "eslint-config-airbnb-typescript-prettier": "^3.1.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.5",
    "typescript": "^3.9.7"
  },
  "prettier": {
    "singleQuote": true,
    "semicolons": true,
    "proseWrap": "always",
    "trailingComma": "all",
    "quoteProps": "consistent",
    "printWidth": 100,
    "tabWidth": 2
  }
}
